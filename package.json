{
  "name": "@socialgouv/fiches-travail-data",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "data",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/fiches-travail-data.git"
  },
  "author": "LionelB <lionel@lumographe.fr>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "mkdir -p dist && cp src/index.js dist/index.esm.js && babel src --out-dir dist --ignore '**/__tests__/**'",
    "start": "node dist/fetch-data",
    "checkRefs": "node dist/checkRefs",
    "lint": "eslint src --ext .js",
    "test": "jest"
  },
  "dependencies": {
    "@socialgouv/datafiller-data": "^1.30.0",
    "esm": "^3.2.25",
    "@socialgouv/legi-data": "^1.18.0",
    "jsdom": "^16.2.2",
    "p-limit": "^3.0.1",
    "superagent": "^5.2.2",
    "talisman": "^1.1.2",
    "unist-util-find": "^1.0.1",
    "unist-util-visit": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@socialgouv/eslint-config-recommended": "^0.30.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src --ext .js",
      "pre-push": "run-p lint test"
    }
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
