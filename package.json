{
  "name": "@socialgouv/fiches-travail-data",
  "version": "0.0.0",
  "main": "index.js",
  "files": [
    "data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/fiches-travail-data.git"
  },
  "author": "LionelB <lionel@lumographe.fr>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel src --out-dir lib --ignore **/__tests__ --extensions .js --extensions .ts",
    "lint": "eslint src --ext .js --ext .ts",
    "start": "node lib/index.js",
    "test": "jest",
    "test:ref-check": "node lib/scripts/checkRefs.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "@socialgouv/datafiller-data": "^1.10.0",
    "@socialgouv/legi-data": "^1.2.0",
    "jsdom": "^16.2.2",
    "p-limit": "^2.3.0",
    "superagent": "^5.2.2",
    "talisman": "^1.1.1",
    "unist-util-find": "^1.0.1",
    "unist-util-visit": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@socialgouv/eslint-config-recommended": "^0.20.0",
    "@types/jest": "^25.2.1",
    "@types/jsdom": "^16.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/data/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p -l type-check lint",
      "pre-push": "run-p -l type-check lint test"
    }
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
